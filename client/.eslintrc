{
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "max-len": [
      "error",
      {
        "code": 200
      }
    ],
    "quotes": [
      "error",
      "double"
    ],
    "react/destructuring-assignment": [
      "never"
    ],
    "react/forbid-prop-types": [
      "disabled"
    ],
    "react/prop-types": [
      1
    ],
    "no-underscore-dangle": [
      0
    ],
    "implicit-arrow-linebreak": "off"
  }
}